{
  "name": "seprice",
  "version": "1.0.0",
  "description": "Sistema integral de gestión clínica para consultorios externos y estudios clínicos",
  "author": "SEPRICE Team <soporte@seprice.com>",
  "private": true,
  "main": "dist/electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000\" \"electron .\"",
    "electron:build": "npm run build && npm run electron:compile && electron-builder",
    "electron:compile": "tsc -p electron/tsconfig.json",
    "electron:start": "npm run build && npm run electron:compile && electron .",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@radix-ui/react-label": "latest",
    "@types/jsonwebtoken": "^9.0.0",
    "better-sqlite3": "latest",
    "child_process": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-is-dev": "latest",
    "electron-serve": "^1.1.0",
    "http": "latest",
    "jsonwebtoken": "latest",
    "lucide-react": "^0.454.0",
    "next": "14.2.16",
    "path": "latest",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "url": "latest"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/semver": "^7.5.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.30",
    "postcss": "^8.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.seprice.clinica",
    "productName": "SEPRICE - Sistema de Gestión Médica",
    "copyright": "Copyright © 2024 SEPRICE",
    "compression": "maximum",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      ".next/**/*",
      "public/**/*",
      "server.js",
      "!node_modules/.cache/**/*",
      "!**/*.map",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "publisherName": "SEPRICE"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "SEPRICE",
      "deleteAppDataOnUninstall": false,
      "runAfterFinish": true,
      "menuCategory": "Medicina",
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "uninstallDisplayName": "${productName}"
    },
    "portable": {
      "artifactName": "${productName}-${version}-Portable.${ext}"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.medical"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Office"
    }
  }
}